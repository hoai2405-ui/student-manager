{
  "version": 2,
  "builds": [
    {
      "src": "fe/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "handle": "filesystem"
    },
    {
      "source": "/(.*)",
      "destination": "fe/$1"
    }

  ]
}
