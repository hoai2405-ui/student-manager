{
  "version": 2,
  "builds": [
    {
      "src": "fe/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/fe/index.html"
    }
  ],
  "static": {
    "directory": "fe/dist"
  }
}
